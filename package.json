  "scripts": {
    "start": "node build/src/server.js",
    "dev": "concurrently --kill-others \"npm run ts\" \"nodemon build/src/server.js\"",
    "ts": "rm -rf build && tsc -w",
    "build": "rm -rf build && tsc --project .",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "npx prettier ./src --write && eslint --fix --ext .ts ./src",
    "migrate": "knex migrate:latest --cwd=build/src",
    "rollback": "knex migrate:rollback --cwd=build/src",
    "seed": "knex seed:run --cwd=build/src",
    "refresh": "npm run rollback && npm run migrate",
    "migrate:local": "npm run build && npm run migrate",
    "rollback:local": "npm run build && npm run rollback",
    "migrate:test": "knex migrate:latest --cwd=src",
    "rollback:test": "knex migrate:rollback --cwd=src",
    "refresh:test": "npm run rollback:test && npm run migrate:test",
    "test": "NODE_ENV=test dotenv -e .env npm run refresh:test && NODE_ENV=test NEW_RELIC_ENABLED=false dotenv -e .env jest"
  },
