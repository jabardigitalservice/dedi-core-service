  "scripts": {
    "start": "node build/server.js",
    "dev": "nodemon src/server.ts",
    "build": "rm -rf build && tsc --project .",
    "lint": "eslint --fix --ext .ts ./src",
    "migrate": "knex migrate:latest --cwd=./src",
